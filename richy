Sub donutShop()
    Dim nod As Integer, bill As Currency
    
    
    'input
    nod = InputBox("How Many donuts would you like")
    
    'processing
    If nod >= 12 Then
        If nod >= 50 Then
            bill = nod * 0.38
        Else
            bill = nod * 0.4
        End If
    Else
        bill = nod * 0.6
    End If
    
    'output
    
    MsgBox ("Your total is: $" & bill)
    
End Sub
